{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "endOfLine": "auto",
  "printWidth": 100,
  "bracketSpacing": true,
  "vueIndentScriptAndStyle": false,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "proseWrap": "always",
  "quoteProps": "as-needed"
}